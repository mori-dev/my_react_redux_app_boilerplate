{
  "name": "react-minimal-boilerplate",
  "version": "1.0.0",
  "description": "``` Boilerplate for React with babel and webpack ```",
  "scripts": {
    "lint": "eslint ./src",
    "start": "webpack-dev-server --progress --color --hot --inline --host 0.0.0.0 --content-base dist/",
    "ncu": "./node_modules/.bin/ncu",
    "ncu:u": "./node_modules/.bin/ncu -u"
  },
  "dependencies": {
    "react": "^15.2.0",
    "react-dom": "^15.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.0.0",
    "eslint-plugin-react": "^5.2.2",
    "npm-check-updates": "^2.6.8",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:mori-dev/react-minimal-boilerplate.git"
  },
  "keywords": [
    "react",
    "webpack",
    "boilerplate",
    "babel"
  ],
  "author": "Kazuhiko Mori <mori.dev.asdf@gmail.com> (https://github.com/mori-dev)",
  "license": "MIT",
  "homepage": "https://github.com/mori-dev/react-minimal-boilerplate"
}
